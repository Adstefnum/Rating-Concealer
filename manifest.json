{
"manifest_version": 2,
    "name": "[Rating Concealer]",
    "description": "[An extension for blocking ratings.]",
    "author": "Stephen Adesina",
    "version": "1.0",
    "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}